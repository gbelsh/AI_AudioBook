<script>

    // Get MG2 subscription status locally.
    var mg2_subscription_status = 'false';
    
    try {
        if (Connext.Storage.GetUserState() == 'Subscribed') {
            mg2_subscription_status = 'true';
        } else {
            mg2_subscription_status = 'false';
        }
    } catch (err) {
        mg2_subscription_status = 'false';
    }
    
    // Declared here because of issues with IE11
    function getParameterByName(name, url) {
        if (!url) {
            url = window.location.href;
        }
        name = name.replace(/[\[\]]/g, "\\$&");
        var regex = new RegExp("[?&]" + name + "(=([^&#]*)|&|#|$)"),
            results = regex.exec(url);
        if (!results) return null;
        if (!results[2]) return '';
        return decodeURIComponent(results[2].replace(/\+/g, " "));
    }
    
    function referNormalizer(refer) {
        refer = refer.replace(/http(s)*:\/\/(www\.)*/g, "");
        refer = refer.replace(/\/.*/g, "");
    
        if (refer == "t.co" || refer == "thech.ca") {
            return "twitter";
        } else if (refer.indexOf(".com") > -1 || refer.indexOf(".ca") > -1) {
            refer = refer.substring(0, refer.lastIndexOf("."));
            if (refer.indexOf(".") > -1) {
                refer = refer.substring(refer.indexOf(".") + 1, refer.length);
            }
        } else if (refer.indexOf(".ly") > -1) {
            refer = refer.replace(".", "");
        }
        return refer;
    }
    jsRefer = referNormalizer(document.referrer);
    
    function getSearchParameters() {
        var prmstr = window.location.search.substr(1);
        return prmstr != null && prmstr != "" ? transformToAssocArray(prmstr) : null;
    }
    
    function getNewsletter() {
    
        try {
            newsletter = getSearchParameters('utm_campaign');
            newsletter = newsletter['utm_campaign'].split("-", 2)[1];
        } catch (err) {
            newsletter = null;
        }
        return newsletter;
    }
    
    var newsletter = getNewsletter();
    
    // https://stackoverflow.com/questions/5448545/how-to-retrieve-get-parameters-from-javascript
    function transformToAssocArray(prmstr) {
        var params = {};
        var prmarr = prmstr.split("&");
        for (var i = 0; i < prmarr.length; i++) {
            var tmparr = prmarr[i].split("=");
            params[tmparr[0]] = tmparr[1];
        }
        return params;
    }
    
    var params = getSearchParameters();
    
    window.dbct = {
        "dfp_preview": getParameterByName('dfp_preview'),
        "keywords": "",
        "referrer": jsRefer,
        "newsletter": newsletter,
        "section": "homepage",
        "subsection": "null",
        "author": "null",
        "page_url": "/cape\u002Dbreton/",
        "story_id": "null",
        "get_params": params,
        "meter": "free",
        "subscriber": mg2_subscription_status,
        "environment": "",
        "slug": 'null',
        "admin-login": 'false',
        "site": "saltwire",
        "ad-tags": "",
        "hb_bidder": "ozone"
    }
    
    window.dbct.publications = "saltwire";
    
    //<!-- Checking to see if the long/lat KV's are set in cookies and if so registering them to the googletag -->

    window.dbct.latitude = "";

    window.dbct.longitude = "";

    //<!-- Checks to see if the location string is empty, which is the default set in the context processor if there is no location set in the cookies -->

    if ("atlantic\u002Dcanada" == '') {
        window.dbct.location = null;
    } else {
        window.dbct.location = "atlantic\u002Dcanada";
    }
    
    var section = 'homepage';
    var sub_section = 'null';

    // define page_id
    var page_id = sessionStorage.getItem("_db_page_id");

    if (!page_id) {
        sessionStorage.setItem("_db_page_id", 1);
    } else {
        if (page_id < 15) {
            sessionStorage.setItem("_db_page_id", parseInt(page_id) + 1);
        }
    }
</script>

    
