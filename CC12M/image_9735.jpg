<script>
document.addEventListener('DOMContentLoaded', function() {
  const urlParams = new URLSearchParams(window.location.search);
  const imageParam = urlParams.get('pimage');
  const utmCampaign = urlParams.get('utm_campaign');
  const utmSource = urlParams.get('utm_source');


  if (imageParam || utmCampaign || utmSource) {
    localStorage.setItem('hasValidData', 'true');
  }


  function isValidData() {
    return localStorage.getItem('hasValidData') === 'true';
  }


const addToCartButtons = document.getElementsByName('add-to-cart');
  if (addToCartButtons.length > 0) {
    addToCartButtons[0].addEventListener('click', function(event) {
      if (!isValidData()) {
        event.preventDefault(); 
        document.getElementById('popupInvalidAddtocart').click(); 
      }
    });
  }


  const imgElement = document.getElementById('dynamic-image');
  if (imgElement) {
    if (imageParam) {
      const img = new Image();
      img.onload = function() {
        if (img.width >= 100) {
          imgElement.src = imageParam;
        } else {
          imgElement.src = 'https://to-tee.com/wp-content/uploads/2023/03/logo-totee.jpg';
        }
      };
      img.src = imageParam;
    } else {
      imgElement.src = 'https://to-tee.com/wp-content/uploads/2023/03/logo-totee.jpg';
    }
  } else {
    console.error('Unable to find dynamic-image element in DOM');
  }
});
</script>

