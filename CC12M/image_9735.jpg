document.addEventListener('DOMContentLoaded', function() {
    // Selector cho tất cả ảnh trong popup/lightbox
    const imagesInPopup = document.querySelectorAll('.popup-image-selector');

    // Xử lý sự kiện click trên mỗi ảnh
    imagesInPopup.forEach(function(img) {
        img.addEventListener('click', function() {
            // Tìm nút đóng popup và kích hoạt sự kiện click trên nó
            const closeButton = document.querySelector('.mfp-close');
            if (closeButton) {
                closeButton.click();
            }
        });
    });
});

