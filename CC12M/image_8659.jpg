					setTimeout(function() {
						body.classList.toggle('countDown');
					}, 4000);
				}

				// Hide the countdown overlay
				function closeCountdown() {
					var body = document.body;

					body.classList.remove('countDown');
					sessionStorage.countdownClosed = false;
				}

				// Open the countdown overlay
				function openCountdown() {

					if (!sessionStorage.countdownClosed) {
						countDown();
					}
				}

				function checkDates() {

					if (Date.parse(deadline) - Date.parse(new Date()) < 0) {
						console.log("past");
					} else {
						openCountdown();
						// initializeClock('clockdiv', deadline);
					}
				}

				checkDates();
			
			// Function to calculate the time remaining from a timestamp given
			function getTimeRemaining(endtime) {
				// Working out each unit of time
				const total = Date.parse(endtime) - Date.parse(new Date());
				const minutes = Math.floor((total / 1000 / 60) % 60);
				const hours = Math.floor((total / (1000 * 60 * 60)) % 24);
				const days = Math.floor(total / (1000 * 60 * 60 * 24));

				return {
					total,
					days,
					hours,
					minutes
				};
			}

			// Function to initialise the countdown timers
			function initializeClock(id, endtime) {
				const clock = document.getElementById(id);
				const daysSpan = clock.querySelector('.days');
				const hoursSpan = clock.querySelector('.hours');
				const minutesSpan = clock.querySelector('.minutes');

				// Function to update the countdown
				function updateClock() {
					const t = getTimeRemaining(endtime);

					daysSpan.innerHTML = ('0' + t.days).slice(-2);
					hoursSpan.innerHTML = ('0' + t.hours).slice(-2);
					minutesSpan.innerHTML = ('0' + t.minutes).slice(-2);

					if (t.total <= 0) {
						clearInterval(timeinterval);
					}
				}

				updateClock();

				const timeinterval = setInterval(updateClock, 1000);
			}
		</script>

