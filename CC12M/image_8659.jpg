<script type="text/javascript">
	var $ = jQuery.noConflict();

	// Initialise Vue
	window.vm['email_signup_right'] = new Vue({
		name: 'Email Signup',
		el: '#email_signup_right',
		store: window.store,
		data: {
			user_id:'',
			endpoint: 'https://www.roseberys.co.uk/' + 'index.php?option=com_account&format=json&task=',
            inputValue: '',
            inputErrors: [],
			// checkedPreferences: < ?php echo json_encode($userObj->getInterests()); ?> ,
			personalisedLotAlerts: null,
            interests:null,
			// Define the progression
			progression: {
				current:1
			},
			registration_for:'',
			interest_data: [
				{
					img:"antiquities-interests.png",
					name:"Antiquities, Islamic & Indian Art"
				},
				{
					img:"ceramics-interests.png",
					name:"Ceramics & Glass"
				},
				{
					img:"chinese-interests.png",
					name:"Chinese, Japanese & Southeast Asian Art"
				},
				{
					img:"dec-arts-interests.png",
					name:"Decorative Arts & Modern Design"
				},
				{
					img:"furniture-interests.png",
					name:"Furniture & Rugs"
				},
				{
					img:"jewellery-interests.png",
					name:"Jewellery & Watches"
				},
				{
					img:"modern-british-interests.png",
					name:"Modern British & 20th Century Art"
				},
				{
					img:"old-master-interests.png",
					name:"Old Master, British & European Pictures"
				},
				{
					img:"silver-interests.png",
					name:"Silver"
				},
				{
					img:"sculpture-interests.png",
					name:"Works of Art, Sculpture & Clocks"
				},
				{
					img:"prints-interests.png",
					name:"Prints & Multiples"
				},
				{
					img:"prints-interests.png",
					name:"Probate, Estates & Insurance Valuations"
				},


				// {
				// 	img:"modern-design-interests.png", // no image
				// 	name:"Impressionist, Modern, Post War & Contemporary Art"
				// },
				// {
				// 	img:"indian-interests.png",
				// 	name:"Islamic & Indian Art"
				// },
				
				
				
			],
			single_keyword: '',
			single_keyword_error: '',
			isCheckAllInterest: false,

			// Define the registration data
			email_signup_right: {
				email: '',
                interests:[],
                keywords:[],
				terms_and_conditions: false,
				email_subscribe:false,
			},
			is_already_registered:false,
			btn_disabled:false,
			btn_disabled_text:"Please wait..",
			is_submit:false,
			step1_error:'',
			step2_error:'',
			show_password_data: false,
			// Define API
			apiurl: 'https://www.roseberys.co.uk/index.php?option=com_user&format=json&task=newsletter.',
			apivurl: 'https://www.roseberys.co.uk/index.php?option=com_user&format=json&task=validate.',
			apioptions: {emulateJSON: true, dataType: "json", contentType: false, processData: false},
			lite_account_created:false,
		},

		created: function () {

		},

		methods: {

			checkAll: function(){
				this.isCheckAllInterest = !this.isCheckAllInterest;
				this.email_signup_right.interests = [];
				if(this.isCheckAllInterest){ // Check all
					for (var key in this.interest_data) {

						this.email_signup_right.interests.push(this.interest_data[key].name);
					}
				}
			},

			updateCheckall:function(){
				if(this.email_signup_right.interests.length == this.interest_data.length){
					this.isCheckAllInterest = true;
				}else{
					this.isCheckAllInterest = false;
				}
			},

			addKeyword:function(){
				delete this.single_keyword_error;
				if(this.single_keyword!==''){
					this.email_signup_right.keywords.push(this.single_keyword);
					this.single_keyword = '';
				}else{
					this.single_keyword_error = "Please enter keyword";
				}
				this.$forceUpdate();
			},

			removeKeyword:function(keyword){
				const new_arr = this.email_signup_right.keywords.filter(key => key!==keyword);
				this.email_signup_right.keywords = new_arr;
				this.$forceUpdate();
			},

			checkEmailAvailability: function () {

				this.$forceUpdate();
				var self = this;
				// Define query
				var query = {
					email: this.email_signup_right.email,
				};

				// Run request
				this.$http.post(this.apivurl + 'checkEmailAvailability', query, this.apioptions).then(function (res) {
					// Check if available
					if (res.body.status === false) {
						this.is_already_registered = true;
						this.$forceUpdate();
					} else {
						this.is_already_registered = false;
					}
				}).catch(function (err) {

				});

			},

			newsletter_signup: function () {
				this.is_submit = true;
				this.btn_disabled = true;
				 
				var self = this;
				// Send the data
				var query = new FormData();
				query.append('user_details', JSON.stringify(this.email_signup_right));
				// Send the request
				this.$http.post(this.apiurl + 'signup', query, this.apioptions).then(function (res) {
					// Check request status
					if (res.body.status==true) {
						self.is_submit= false;
						self.btn_disabled = false;
						if(self.is_already_registered){
							self.progression.current = 3;
						}else{
							self.progression.current = 2;
						}
					} else {
						self.is_submit = false;
						self.btn_disabled = false;
                        if ((res.body.message).length >= 0) {
                            self.step1_error = res.body.message;
                        }else{
                            self.step1_error = "There was an error while submitting your choices, please contact the auctioneer to rectify this issue1.";
                        }
					}
				}).catch(function (err) {
					self.is_submit= false;
					self.btn_disabled = false;
					self.step1_error = "There was an error while submitting your choices, please contact the auctioneer to rectify this issue2.";
				});
				this.$forceUpdate();
			},

			createLiteAccount: async function(){
				var self = this;
				// Send the data
				this.btn_disabled = true;
				var query = new FormData();
				query.append('user_details', JSON.stringify(this.email_signup_right));
				// Send the request
				await this.$http.post(this.apiurl + 'registerLiteAccount', query, this.apioptions).then(function (res) {

					// Check request status
					if (res.body.status) {
						self.user_id = res.body.user_id;
						self.lite_account_created = true;
						self.progression.current++;
						self.btn_disabled = false;
					} else {
					    self.step2_error = res.body.message;
						self.btn_disabled = false;
					}
				}).catch(function (err) {
					self.btn_disabled = false;
				});

				this.$forceUpdate();
			},

			checkAndProceed: async function () {
				 
				// event.preventDefault();
				switch (this.progression.current) {
					case 1:
						// Define failed
						var failed_validation = false;

						// Check email
						delete this.email_signup_right.email_error;
						if (this.email_signup_right.email=='') {
							failed_validation = true;
							this.email_signup_right.email_error = 'Please enter email';
						}
						var email_validate = /^(([^<>()[\]\\.,;:\s@\"]+(\.[^<>()[\]\\.,;:\s@\"]+)*)|(\".+\"))@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\])|(([a-zA-Z\-0-9]+\.)+[a-zA-Z]{2,}))$/;
						if (this.email_signup_right.email!='' && !email_validate.test(this.email_signup_right.email)) {
							failed_validation = true;
							this.email_signup_right.email_error = 'Your email address is not valid';
						}

						delete this.email_signup_right.interests_error;
                        if (this.email_signup_right.interests.length ===0) {
                            failed_validation = true;
                            this.email_signup_right.interests_error = 'Please select any option';
                        }

						// check email is available or not
						this.checkEmailAvailability();

						// Check if failed validation
						if (!failed_validation) {
							// this.progression.current++;
							this.newsletter_signup();
						}
						window.scrollTo(0, 0);
						break;

					case 2:
                        // Define failed
						var failed_validation = false;

						// delete this.email_signup_right.first_name_error;
						// if (this.email_signup_right.first_name.length < 2) {
						// 	failed_validation = true;
						// 	this.email_signup_right.first_name_error = 'Please enter your first name';
						// }

						// Check last name
						// delete this.email_signup_right.last_name_error;
						// if (this.email_signup_right.last_name.length < 2) {
						// 	failed_validation = true;
						// 	this.email_signup_right.last_name_error = 'Please enter your last name';
						// }

						// delete this.email_signup_right.phone_no_error;
                        // if (this.email_signup_right.phone_no === '') {
                        //     failed_validation = true;
                        //     this.email_signup_right.phone_no_error = 'Please enter phone no';
                        // }

						// Check email
						delete this.email_signup_right.email_error;
						if (this.email_signup_right.email=='') {
							failed_validation = true;
							this.email_signup_right.email_error = 'Please enter email';
						}
						var email_validate = /^(([^<>()[\]\\.,;:\s@\"]+(\.[^<>()[\]\\.,;:\s@\"]+)*)|(\".+\"))@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\])|(([a-zA-Z\-0-9]+\.)+[a-zA-Z]{2,}))$/;
						if (this.email_signup_right.email!='' && !email_validate.test(this.email_signup_right.email)) {
							failed_validation = true;
							this.email_signup_right.email_error = 'Your email address is not valid';
						}

						delete this.email_signup_right.password_error;
						var password_validate = /^(?=.*?[A-Z])(?=.*?[a-z])(?=.*?[0-9])(?=.*?[#?!@$%^&*-]).{8,}$/;
						if (!password_validate.test(this.email_signup_right.password)) {
							failed_validation = true;
							this.email_signup_right.password_error = 'Your password must contain: an upper case letter, a lower case letter, a number, a special character and must be 8 characters or more';
						}

						// Check confirm password
						delete this.email_signup_right.confirm_password_error;
						if (this.email_signup_right.confirm_password !== this.email_signup_right.password) {
							failed_validation = true;
							this.email_signup_right.confirm_password_error = 'Both provided passwords don\'t match';
						}

						delete this.email_signup_right.terms_and_conditions_error;
                        if (this.email_signup_right.terms_and_conditions == false) {
                            failed_validation = true;
                            this.email_signup_right.terms_and_conditions_error = 'Please accept terms and conditions';
                        }

                        // Check if failed validation
                        if (!failed_validation) {
							await this.createLiteAccount();
						}
						window.scrollTo(0, 0);
                        break; 
					default:
					        alert("Wrong Step");

				}
				 
				this.$forceUpdate();
			},



            updatePref: function(value) {

                this.$http.post(
                    this.endpoint + 'account.updateEmailPreferences',
                    {
                        interests: this.interest_data,
                    },
                    {
                        emulateJSON: true
                    }
                ).then(function(response) {
                    // Handle response
                    if ( response.data.status ){
                        console.log('success');
                    } else {
                    }

                }, function() {
                });
            },

			changePasswordView: function() {							
				this.show_password_data = !this.show_password_data;			
			},

		},

		mounted: function () {

		   if(sessionStorage.getItem("newsignup_email")){
			 this.email_signup_right.email = sessionStorage.getItem("newsignup_email");
			 sessionStorage.removeItem("newsignup_email")
		   }

		   if(this.registration_for != "email_signup_right"){
			  this.progression.current = 1;
		   }

		},
	});

