<!-- Initialise Vue -->
<script type="text/javascript">

	// Define vue instance
	window.vm['notifications'] = new Vue({
		el: '#notifications-manager',
		name: 'Notifications',
		store: window.store,
		computed: {
			getNotifications: {
				cache: false,
				get: function() {
					var items = JSON.parse(JSON.stringify(this.$store.state.notifications));
					var len = items.length - 1;
					var nitems = [];
					if (items.length <= 3) {
						return items;
					} else {
						nitems.push(items[len]);
						nitems.push(items[len-1]);
						nitems.push(items[len-2]);
						return nitems;
					}
				},
			},
		},

		methods: {
			// Removing the notifications if you can click the close icon
			closeNotification: function() {
				// Removing the notifications
				this.$store.state.notifications = [];
			},
		}
	});
</script>

