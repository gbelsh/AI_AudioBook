<!-- Register to bid live -->
<script>

	jQuery(".btnRegisterToBidLiveRibbon").on('click',function (e) {
		
		jQuery(this).text("Please wait...");
		jQuery(this).attr("disabled","disabled");
		var auction_id = jQuery(this).attr("data-id");
		
		jQuery.ajax({
			type: 'POST',
			url: 'https://www.roseberys.co.uk/index.php?option=com_bidding&format=json&task=commission.check_bid_live_approval',
			dataType: 'JSON',
			data: {
				auction_id: auction_id
			},
			success: function(response) {
				if (response.status) {
					jQuery("#bid_live_approval_message_ribbon").modal('toggle');
					jQuery("#msg_approval_ribbon").text(response.message);

				} else {
					jQuery("#bid_live_approval_message_ribbon").modal('toggle');
					jQuery("#msg_approval_ribbon").text(response.message);
				}
			}
		}).fail(function(error) {
			console.log('Error: ' + error.statusText);
		});

		jQuery(".return-lot-details").on('click',function(e){
			window.location.reload(true);
		});

	});

</script>		
