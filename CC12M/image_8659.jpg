<div class="notifications-container" id="notifications-manager" v-if="getNotifications.length > 0" v-cloak>
	<ul class="notification-items">
		<li v-for="(item, index) in getNotifications" class="notification-item" :class="['ntype-' + item.type]">
			<span class="icon">
				<i v-if="item.type == 'success'" class="far fa-check-circle"></i>
				<i v-if="item.type == 'notice'" class="fas fa-info-circle"></i>
				<i v-if="item.type == 'warning'" class="fas fa-exclamation-triangle"></i>
				<i v-if="item.type == 'error'" class="fas fa-times"></i>
			</span>
			<p class="message" v-text="item.message"></p>

